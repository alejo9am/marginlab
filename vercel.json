{
  "functions": {
    "api/**/*.php": {
      "runtime": "vercel-php@0.7.1"
    }
  },
  "routes": [
    {
      "src": "/(css|img|javascript)/(.*)",
      "dest": "/public/$1/$2"
    },
    {
      "src": "/actions/(.*)",
      "dest": "/api/actions/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ],
  "env": {
    "APP_ENV": "production"
  }
}